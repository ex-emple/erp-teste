# Variables d'environnement pour Railway
# Ces variables seront configurées dans l'interface Railway

# Clé secrète Django (sera générée automatiquement par Railway)
SECRET_KEY=django-insecure-votre-cle-secrete-super-longue-et-aleatoire

# Mode Debug (False en production)
DEBUG=False

# Module de settings pour Railway
DJANGO_SETTINGS_MODULE=config.settings_railway

# Niveau de log Django
DJANGO_LOG_LEVEL=INFO

# Port (Railway le définit automatiquement)
PORT=8000

# NOTE: DATABASE_URL est fournie automatiquement par Railway
# quand vous ajoutez un service PostgreSQL
